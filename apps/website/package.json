  "description": "Gatsby website for TEST",
  "version": "0.7.51",
  "author": "Amazee Labs <developers@amazeelabs.com>",
  "dependencies": {
    "dotenv": "^10.0.0",
    "gatsby": "^4.25.7",
    "gatsby-plugin-schema-export": "^1.1.31",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.43",
    "@amazeelabs/jest-preset": "^1.3.47",
    "@amazeelabs/prettier-config": "^1.1.3",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@types/classnames": "^2.3.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.37",
    "eslint": "^8.43.0",
    "gatsby-source-filesystem": "^4.25.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "update-schema": "GATSBY_UPDATE_SCHEMA_SNAPSHOT=true gatsby build",
    "codegen": "graphql-codegen --config codegen.yml",
    "prepare": "yarn codegen",
    "precommit": "lint-staged",
    "test": "yarn test:static && yarn test:unit && yarn test:integration",
    "watch": "jest --watch",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\" --fix",
    "test:unit": "jest --passWithNoTests",
    "test:integration": "if [ -f test.sh ]; then ./test.sh; fi",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://localhost:4873",
    "repository": "git@github.com:AmazeeLabs/gatsby-starter.git",
    "branch": "main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/silverback-mono"
  },
  "bugs": {
